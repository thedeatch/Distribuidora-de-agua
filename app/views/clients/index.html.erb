  Email <%= client.email %>
  Telefono <%= client.phone%>
  Direccion <%= client.address%>

  <%=link_to 'Mostrar', cliente_url(client) %>
  <%= link_to 'Editar', editar_cliente_url(client) %>
   <%= link_to 'Eliminar', cliente_path(client),
                method: :delete,
                data: {confirm: 'Â¿Estas seguro?'} %>
  <br>
<%end %>
